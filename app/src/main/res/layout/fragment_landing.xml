    <FrameLayout
        android:elevation="4dp"
        android:id="@+id/landing_pageFAB"
        android:background='@drawable/main_fab'
        style="@style/FABButtons"
        >

        <ImageView
            style="@style/MainFabImage"
            android:src="@drawable/pelican_white" />

    </FrameLayout>

</FrameLayout>
