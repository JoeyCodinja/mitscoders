<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

     <android.support.design.widget.TabLayout
         android:id="@+id/landing_tabs"
         style="@style/MyCustomTabLayout"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:background="#ffe20502"
         />

     <android.support.v4.view.ViewPager
         android:id="@+id/landing_viewpager"
         android:layout_width="match_parent"
         android:layout_height="fill_parent"/>


    <ImageView
        android:id="@+id/landing_pageFAB1"
        style="@style/FABButtons"
        android:src="@drawable/campusinfo_fab"/>

    <ImageView
        android:id="@+id/landing_pageFAB2"
        style="@style/FABButtons"
        android:src="@drawable/transportation_fab"/>

    <ImageView
        android:id="@+id/landing_pageFAB3"
        style="@style/FABButtons"
        android:src="@drawable/ourvle_fab"/>

    <ImageView
        android:id="@+id/landing_pageFAB4"
        style="@style/FABButtons"
        android:src="@drawable/boss_fab"/>

    <ImageView
        android:id="@+id/landing_pageFAB5"
        style="@style/FABButtons"
        android:src="@drawable/sas_fab" />

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end">

        <ImageView
            android:id="@+id/landing_pageFAB"
            style="@style/FABButtons"
            android:src="@drawable/main_fab"/>


        <ImageView
            style="@style/MainFabImage"
            android:src="@drawable/pelican_white"/>

    </FrameLayout>

</FrameLayout>
